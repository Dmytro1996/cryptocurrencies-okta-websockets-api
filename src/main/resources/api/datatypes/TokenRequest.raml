#%RAML 1.0 DataType

type: object
properties:
  email:
    type: string
    required: true
    example: "bob@mail.com"
  password:
    type: string
    required: true
    example: "12345abcd"
  scope:
    type: string
    required: true
    example: "okta.apiTokens.read"
  redirectUri:
    type: string
    required: false
    example: "https://localhost:8081/callback"